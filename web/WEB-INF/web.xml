<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AutenticaLogin</servlet-name>
        <servlet-class>br.javaweb.web.AutenticaLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioNovo</servlet-name>
        <servlet-class>br.javaweb.web.UsuarioNovo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReservaNova</servlet-name>
        <servlet-class>br.javaweb.web.ReservaNova</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>br.javaweb.web.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AutenticaLogin</servlet-name>
        <url-pattern>/AutenticaLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioNovo</servlet-name>
        <url-pattern>/UsuarioNovo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReservaNova</servlet-name>
        <url-pattern>/ReservaNova</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            5
        </session-timeout>
    </session-config>
</web-app>
